<%- include('includes/head.ejs') %>
<link rel="stylesheet" href="/css/artisticEvent.css">
<title id="title">Event Name</title>
</head>

<body>
    <%- include('includes/navbar.ejs') %>

    <nav class="crumb" aria-label="breadcrumb">
        <ol class="breadcrumb crumb">
            <li class="breadcrumb-item" aria-current="page"><a class="bc-item" href="#">Home</a></li>
            <li class="breadcrumb-item" aria-current="page"><a class="bc-item" href="#">Calendar</a></li>
            <li id="breadCrumbName" class="breadcrumb-item bc-active" aria-current="page">Event's Name</li>
        </ol>
    </nav>

    <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img id="img1" class="d-block w-100" alt="First slide">
            </div>
            <div class="carousel-item">
                <img id="img2" class="d-block w-100" alt="Second slide">
            </div>
        </div>
        <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>

    <div class="container">
        <div class="d-flex titleDiv flex-row justify-content-center">
            <h1 id="titleLabel" class="Display-1 titleLabel wheat-item">Nome evento</h1>
        </div>
    </div>
    <hr />

    <div class="container">
        <div class="d-flex flex-row justify-content-center">
            <div class="centralized artist-wrapper">
                <h2 class="wheat-item">Featuring:</h2>
                <div id="featuringBox" class="row d-flex justify-content-center">

                </div>
            </div>
        </div>

        <div class="d-flex flex-row justify-content-center">
            <div class="centralized artist-wrapper">
                <h2 class="wheat-item">Other Events The Same Day:</h2>
                <div id="eventsContainer" class="row d-flex justify-content-center">

                </div>
            </div>
        </div>

        <div class="d-flex flex-row justify-content-center">
            <div id="seminarContainer">

            </div>
        </div>

        <div class="d-flex flex-row justify-content-center">
            <div class="abstract centralized">
                <h2 class="display-2"> Details on This Event</h3>
                    <p class="abstractLabel" id="abstract">

                    </p>
            </div>
        </div>

        <hr />
        <div class="row">
            <div class="col-1"></div>
            <div class="col-10 align-items-center">
                <div class="card">
                    <div id="q0" class="card-header" panel-id="question-0">Panel Heading</div>
                    <div id="question-0" class="card-body collapse">
                        Content
                    </div>
                    <div id="q1" class="card-header" panel-id="question-1">Panel Heading</div>
                    <div id="question-1" class="card-body collapse">
                        Content
                    </div>
                    <div id="q2" class="card-header" panel-id="question-2">Panel Heading</div>
                    <div id="question-2" class="card-body collapse">
                        Content
                    </div>
                    <div id="q3" class="card-header" panel-id="question-3">Panel Heading</div>
                    <div id="question-3" class="card-body collapse">
                        Content
                    </div>
                    <div id="q4" class="card-header" panel-id="question-4">Panel Heading</div>
                    <div id="question-4" class="card-body collapse">
                        Content
                    </div>
                </div>
            </div>
            <div class="col-1"></div>
        </div>
        <div id="bottomContainer" class="row">

        </div>
        <div id="bottomPage"></div>
        
    </div>
    <%- include('includes/footer.ejs') %>
    
    <script src="/js/script.js"></script>
    <script src="/js/artisticEvent.js"></script>
</body>

</html>

